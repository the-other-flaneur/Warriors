{
    "compilerOptions": {
        "target": "es2020",
        "module": "ESNext",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "sourceMap": true,
        "moduleResolution": "node",
    },
    "include": ["src/**/*.ts"],
}